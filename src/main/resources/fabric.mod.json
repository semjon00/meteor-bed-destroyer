  "authors": [
    "semjon00"
  ],
  "contact": {
    "sources": "https://github.com/semjon00"
  },

  "license": "GPL-3.0",
  "icon": "assets/beddestroyer/icon.png",

  "environment": "*",
  "entrypoints": {
