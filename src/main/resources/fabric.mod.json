    ]
  },
  "mixins": [
    "beddestroyer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "meteor-client": ">=0.4.9"
  },
  "suggests": {
  },
  "custom": {
    "meteor-client:color": "237,19,19"
